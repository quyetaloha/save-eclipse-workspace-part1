<taglib>
	<tlib-version>1.2</tlib-version>
	<jsp-version>2.5</jsp-version>
	<short-name>Hello tag</short-name>
<tag>
	<name>hello</name>
	<tag-class>com.trungtamjava.taglib.HelloTag</tag-class>
	<body-content>scriptless</body-content>
	<attribute>
		<name>message</name>
		<require>false</require>
		<type>java.util.String</type>
	</attribute>
</tag>
</taglib>